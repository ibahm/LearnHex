<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studyprofile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".chat_fragment_chat"
            android:label="@string/title_activity_chat_fragment_chat"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".chat_notification_row"
            android:label="@string/title_activity_chat_notification_row"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".chat_custom_lastchat_row"
            android:label="@string/title_activity_chat_custom_lastchat_row"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".chat_custom_friend_list_row"
            android:label="@string/title_activity_chat_custom_friend_list_row"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".chat_notifications"
            android:label="@string/title_activity_chat_notifications"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".chat_chat"
            android:label="@string/title_activity_chat_chat"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".chat_message_area"
            android:label="@string/title_activity_chat_message_area"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Login" />
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>